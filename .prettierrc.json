{
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "singleAttributePerLine": true,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^@/i18n",
    "^@/helpers",
    "^@/components",
    "^@/composables",
    "^@/scss"
  ],
  "importOrderSortSpecifiers": true
}